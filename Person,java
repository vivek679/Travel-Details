public class Person{
    private String name;
    private char gender;
    private int age;
 /*   
    Person(){//No argument constructor    
    }
    Person(String name,char gender, int age){//Parameterize constructor
        this.name=name;
        this.gender=gender;
        this.age=age;
    }
  */ 
  
    public String getName(){    return name;    }
    public char getGender(){        return gender;    }
    public int getAge(){        return age;    }
    
    public void setName(String name){
        this.name=name;
    }
    public void setGender(char gender){
        this.gender=gender;
    }
    public void setAge(int age){
        this.age=age;
    }
}
